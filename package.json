{
  "name": "essence-plus",
  "version": "1.0.0",
  "private": false,
  "description": "A lightweight Vue 3 component library for building UI faster.",
  "keywords": [
    "vue3",
    "component",
    "UI",
    "essence",
    "form",
    "button"
  ],
  "author": "Your Name <your@email.com>",
  "license": "MIT",
  "homepage": "https://github.com/yourname/essence-plus",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourname/essence-plus.git"
  },
  "type": "module",
  "main": "./dist/essence-plus.umd.js",
  "module": "./dist/essence-plus.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/essence-plus.es.js",
      "require": "./dist/essence-plus.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./essence-plus.css": "./dist/essence-plus.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": [
    "./dist/style.css",
    "./dist/essence-plus.css"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build --config vite.build.config.ts",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --fix",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@popperjs/core": "^2.11.8",
    "async-validator": "^4.2.5",
    "lodash-es": "^4.17.21"
  },
  "peerDependencies": {
    "vue": "^3.5.0"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.13.9",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vitepress-demo-preview/component": "^2.3.2",
    "@vitepress-demo-preview/plugin": "^1.4.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.21.0",
    "eslint-plugin-vue": "^10.0.0",
    "postcss-color-mix": "^1.1.0",
    "postcss-each": "^1.1.0",
    "postcss-each-variables": "^0.3.0",
    "postcss-for": "^2.1.1",
    "postcss-nested": "^7.0.2",
    "three": "^0.124.0",
    "typescript": "~5.8.0",
    "vanta": "^0.5.24",
    "vite": "^6.2.1",
    "vite-plugin-dts": "^4.5.3",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vitepress": "^1.6.3",
    "vitest": "^3.0.8",
    "vue-tsc": "^2.2.8"
  }
}
